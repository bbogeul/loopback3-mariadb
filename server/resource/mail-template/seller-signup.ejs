<div
  style="margin:0;padding:0;font-family:Helvetica,AppleSDGothicNeo-Regular,Malgun Gothic,맑은고딕,돋움,dotum,sans-serif;background-color:#f8f8fb;width:100%;margin-left: auto;margin-right: auto;"
>
  <table
    width="100%"
    cellpadding="0"
    cellspacing="0"
    align="center"
    border="0"
    style="max-width:600px;margin:0 auto;border-collapse:collapse;font-family:Helvetica,AppleSDGothicNeo-Regular,Malgun Gothic,맑은고딕,돋움,dotum,sans-serif;color:#000000"
  >
    <tr>
      <td height="40"></td>
    </tr>
    <tr>
      <td>
        <table
          width="100%"
          cellpadding="0"
          cellspacing="0"
          align="center"
          border="0"
          bgcolor="#ffffff"
          style="max-width:600px;width:100%;border-collapse:collapse;font-family:Helvetica,AppleSDGothicNeo-Regular,Malgun Gothic,맑은고딕,돋움,dotum,sans-serif;color:#000000"
        >
          <tr>
            <td border="0" cellpadding="0" cellspacing="0" height="4" bgcolor="#000000"></td>
          </tr>
          <tr>
            <td height="100" align="center" valign="middle">
              <img src="https://d3ea9molln0us0.cloudfront.net/img/static/mail/mail_logo.png" />
            </td>
          </tr>
          <tr>
            <td height="1" bgcolor="#cccccc"></td>
          </tr>
          <tr>
            <td height="5" bgcolor="#f8f8fb"></td>
          </tr>
          <tr>
            <td height="49"></td>
          </tr>
          <tr>
            <td>
              <table
                width="100%"
                cellpadding="0"
                cellspacing="0"
                align="center"
                border="0"
                bgcolor="#ffffff"
                style="font-family:Helvetica,AppleSDGothicNeo-Regular,Malgun Gothic,맑은고딕,돋움,dotum,sans-serif;color:#000000"
              >
                <tr>
                  <td width="30"></td>
                  <td>
                    <table
                      width="100%"
                      cellpadding="0"
                      cellspacing="0"
                      align="center"
                      border="0"
                      bgcolor="#ffffff"
                      style="font-family:Helvetica,AppleSDGothicNeo-Regular,Malgun Gothic,맑은고딕,돋움,dotum,sans-serif;color:#000000"
                    >
                      <tr>
                        <td>
                          <p style="line-height:26px;font-size:26px;text-align:center;">Your have successfully registered!</p>
                        </td>
                      </tr>
                    </table>

                    <table
                      width="100%"
                      cellpadding="0"
                      cellspacing="0"
                      align="center"
                      border="0"
                      bgcolor="#ffffff"
                      style="font-family:Helvetica,AppleSDGothicNeo-Regular,Malgun Gothic,맑은고딕,돋움,dotum,sans-serif;color:#000000"
                    >
                      <tr>
                        <td colspan="3" height="5"></td>
                      </tr>
                      <tr>
                        <td>
                          <table
                            width="100%"
                            cellpadding="0"
                            cellspacing="0"
                            align="center"
                            border="0"
                            style="font-family:Helvetica,AppleSDGothicNeo-Regular,Malgun Gothic,맑은고딕,돋움,dotum,sans-serif;color:#000000"
                          >
                            <tr>
                              <td></td>
                              <td width="100" height="100" align="center" valign="middle">
                                <div style="max-width:100px;max-height:100px;border-radius:100px;">
                                  <img src="<%= avatarUrl %>" style="display:inline-block;max-width:100px;border-radius:100px" />
                                </div>
                              </td>
                              <td></td>
                            </tr>
                          </table>
                        </td>
                      </tr>
                      <tr>
                        <td height="7"></td>
                      </tr>
                      <tr>
                        <td>
                          <p style="line-height:16px;font-size:16px;text-align:center;"><b>Hello! <%= writerName %></b></p>
                        </td>
                      </tr>
                      <% if(email) { %>
                      <tr>
                        <td align="center"><p style="line-height:14px;word-wrap:break-word;font-size:12px"><%= email %></p></td>
                      </tr>
                      <% } %>
                      <tr>
                        <td height="28"></td>
                      </tr>
                    </table>

                    <table
                      width="100%"
                      cellpadding="0"
                      cellspacing="0"
                      align="center"
                      border="0"
                      bgcolor="#fafafa"
                      style="font-family:Helvetica,AppleSDGothicNeo-Regular,Malgun Gothic,맑은고딕,돋움,dotum,sans-serif;color:#000000;padding: 10px 20px 20px 20px;"
                    >
                      <tr>
                        <td height="10"></td>
                      </tr>
                      <tr>
                        <td>
                          <span style="line-height:16px;word-wrap:break-word;font-size:14px">
                            <b>Your sign up request has been successfully registered!</b><br />
                            <b>Your account will be verified and authenticated before approval.</b><br />
                            <b>Wait for feedback to be given within a maximum of seven days.</b><br />
                          </span>
                        </td>
                      </tr>
                      <tr>
                        <td height="15"></td>
                      </tr>
                      <tr>
                        <td height="10"></td>
                      </tr>
                      <tr>
                        <td>
                          <span style="line-height:24px;word-wrap:break-word;font-size:12px">Message Sent @ <%= created %></span>
                        </td>
                      </tr>
                    </table>
                  </td>
                  <td width="30"></td>
                </tr>
              </table>
            </td>
          </tr>
          <tr>
            <td height="40"></td>
          </tr>
          <tr>
            <td height="1" bgcolor="#cccccc"></td>
          </tr>
          <tr>
            <td height="5" bgcolor="#f8f8fb"></td>
          </tr>
          <tr>
            <td height="105">
              <table
                width="100%"
                cellpadding="0"
                cellspacing="0"
                align="center"
                border="0"
                bgcolor="#ffffff"
                style="font-family:Helvetica,AppleSDGothicNeo-Regular,Malgun Gothic,맑은고딕,돋움,dotum,sans-serif;color:#000000"
              >
                <tr>
                  <td align="center" valign="middle">
                    <img src="https://d3ea9molln0us0.cloudfront.net/img/static/mail/ico_mail.png" />
                  </td>
                </tr>
                <tr>
                  <td height="10"></td>
                </tr>
                <tr>
                  <td align="center" valign="middle">
                    <p style="font-size:12px;color:#666666">Copyright @ 2019 umma, All rights reserved.</p>
                  </td>
                </tr>
              </table>
            </td>
          </tr>
          <tr>
            <td height="1" bgcolor="#cccccc"></td>
          </tr>
        </table>
      </td>
    </tr>
    <tr>
      <td height="40"></td>
    </tr>
  </table>
</div>
