{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 4000,
  "legacyExplorer": false,
  "aclErrorStatus": 403,
  "remoting": {
    "types": {
      "warnOnUnknownType": false
    },
    "context": false,
    "rest": {
      "normalizeHttpPath": true,
      "xml": false,
      "handleErrors": false
    },
    "json": {
      "strict": false,
      "limit": "10mb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "10mb"
    },
    "cors": false,
    "handleErrors": false
  }
}
